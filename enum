enum Grade {
    A, B, C;
}

public class App {
    public static void main(String args[]) {
        Grade myGrade = Grade.B;

        switch(myGrade) {
            case A:
                System.out.println(">90");
                break;

            case B:
                System.out.println(">70");
                break;

            case C:
                System.out.println(">50");
                break;

            default:
                System.out.println("failed");
        }

        for (Grade myVar : Grade.values()) {
            System.out.println(myVar);
        }
    }
}
